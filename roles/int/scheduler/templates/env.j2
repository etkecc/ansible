SCHEDULER_LOGIN={{ int_scheduler_login }}
SCHEDULER_PASSWORD={{ int_scheduler_password }}
SCHEDULER_HOMESERVER={{ int_scheduler_homeserver }}
SCHEDULER_DB_DSN={{ int_scheduler_database_path_in_container }}
SCHEDULER_SENTRY={{ int_scheduler_sentry }}
SCHEDULER_LOGLEVEL={{ int_scheduler_loglevel }}
{% set commands = [] %}
{% for command in int_scheduler_commands -%}{{- commands.append(command.name) -}}
SCHEDULER_{{ command.name|upper }}_CMD={{ command.cmd|default('') }}
SCHEDULER_{{ command.name|upper }}_DESCRIPTION={{ command.description|default('') }}
SCHEDULER_{{ command.name|upper }}_LOG={{ command.log|default('') }}
{% endfor %}
SCHEDULER_LIST={{ commands|join(" ") }}

{{ int_scheduler_environment_variables_extension }}
