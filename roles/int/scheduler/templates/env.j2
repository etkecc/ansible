SCHEDULER_LOGIN={{ int_scheduler_login }}
SCHEDULER_PASSWORD={{ int_scheduler_password }}
SCHEDULER_HOMESERVER={{ int_scheduler_homeserver }}
SCHEDULER_ENV={{ int_scheduler_env }}
SCHEDULER_ROOMID={{ int_scheduler_roomid }}
SCHEDULER_DB_DSN={{ int_scheduler_database_path_in_container }}
SCHEDULER_SENTRY_DSN={{ int_scheduler_sentry_dsn }}
SCHEDULER_SENTRY_RATE={{ int_scheduler_sentry_rate }}
SCHEDULER_LOGLEVEL={{ int_scheduler_loglevel }}
SCHEDULER_WORKERS={{ int_scheduler_workers }}
SCHEDULER_RECURRING_ENABLED={{ int_scheduler_recurring_enabled }}
SCHEDULER_RECURRING_LISTCOMMAND={{ int_scheduler_recurring_listcommand }}
SCHEDULER_RECURRING_MXID={{ int_scheduler_recurring_mxid }}
SCHEDULER_RECURRING_ROOMID={{ int_scheduler_recurring_roomid }}
SCHEDULER_RECURRING_EVENTID={{ int_scheduler_recurring_eventid }}
SCHEDULER_RECURRING_COMMAND={{ int_scheduler_recurring_command }}
{% set commands = [] %}
{% for command in int_scheduler_commands -%}{{- commands.append(command.name) -}}
SCHEDULER_{{ command.name|upper }}_CMD={{ command.cmd|default('') }}
SCHEDULER_{{ command.name|upper }}_DESCRIPTION={{ command.description|default('') }}
SCHEDULER_{{ command.name|upper }}_LOG={{ command.log|default('') }}
SCHEDULER_{{ command.name|upper }}_MIGRATE={{ command.migrate|default('') }}
SCHEDULER_{{ command.name|upper }}_LOGONERROR={{ command.logonerror|default('') }}
{% endfor %}
SCHEDULER_LIST={{ commands|join(" ") }}
{% set admin_commands = [] %}
{% for command in int_scheduler_admin_commands -%}{{- admin_commands.append(command.name) -}}
SCHEDULER_ADMIN_{{ command.name|upper }}_CMD={{ command.cmd|default('') }}
SCHEDULER_ADMIN_{{ command.name|upper }}_DESCRIPTION={{ command.description|default('') }}
SCHEDULER_ADMIN_{{ command.name|upper }}_LOG={{ command.log|default('') }}
SCHEDULER_ADMIN_{{ command.name|upper }}_MIGRATE={{ command.migrate|default('') }}
{% endfor %}
SCHEDULER_ADMINLIST={{ admin_commands|join(" ") }}

{{ int_scheduler_environment_variables_extension }}
